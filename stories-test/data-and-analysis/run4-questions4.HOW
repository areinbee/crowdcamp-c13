## Retrieve
${MTURKPY} retrieve "${HITID}" | head -2 | tee "${BASEDIR}/results.csv.meta"
${MTURKPY} retrieve "${HITID}" | tail -n +3 > "${BASEDIR}/results.csv"
./csv_helpers.py "${BASEDIR}/results.csv" payload > "${BASEDIR}/results-payload.json"

